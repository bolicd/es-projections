<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/tacta-io/es-projections</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/tacta-io/es-projections/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/tacta-io/es-projections</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Provides a simple way of incrementally building / rebuilding read model projections for .NET projects</Description>
    <PackageIconUrl>https://tacta.io//wp-content/uploads/2018/10/tacta_logo_neg.png</PackageIconUrl>
    <PackageId>ESProjections</PackageId>
  </PropertyGroup>

</Project>
